<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RepositoryUrl>https://github.com/NotCoffee418/CSharpScriptOperations</RepositoryUrl>
    <PackageProjectUrl>https://github.com/NotCoffee418/CSharpScriptOperations</PackageProjectUrl>
    <RepositoryType></RepositoryType>
    <PackageTags>csharp, scripting, console</PackageTags>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Description>Tool for running blocks of code for scripting long-running operations in a console application.</Description>
    <Authors>NotCoffee418</Authors>
    <Version>1.3.0</Version>
    <PackageReleaseNotes>Operation description is now an attribute instead of a property to work better with dependency injection</PackageReleaseNotes>
	<LangVersion>latest</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Autofac" Version="6.3.0" />
    <PackageReference Include="Autofac.Extensions.DependencyInjection" Version="7.2.0" />
  </ItemGroup>

</Project>
